{
  "policy": {
    "actions": [
      {
        "category": 2,
        "type": "event"
      }
    ],
    "conditions": [
      {
        "analytic": "baselineDeviation",
        "metric": "windows_events.event_count",
        "operator": ">"
      }
    ],
    "deleted": false,
    "description": "The number of Windows Events being generated is higher than expected based on historical behavior.",
    "duration": 900,
    "enabled": true,
    "name": "Windows - Elevated Event Count",
    "scope": {
      "elementTypes": [
        "WINSRV"
      ]
    }
  }
}